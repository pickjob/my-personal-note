{
    "inbounds": [
        {
            "protocol": "vmess",
            "listen": "0.0.0.0",
            "port": 12345,
            "settings": {
                "clients": [
                    {
                        "id": "uuid",
                        "alterId": 1
                    }
                ],
                "disableInsecureEncryption": false
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {}
        }
    ],
    "log": {
        "access": "/var/log/v2ray/access.log",
        "error": "/var/log/v2ray/error.log",
        "loglevel": "debug"
    }
}